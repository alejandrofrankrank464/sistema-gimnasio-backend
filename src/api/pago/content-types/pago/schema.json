{
  "kind": "collectionType",
  "collectionName": "pagos",
  "info": {
    "singularName": "pago",
    "pluralName": "pagos",
    "displayName": "Pago"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "Monto": {
      "type": "decimal",
      "required": true,
      "min": 0
    },
    "MetodoPago": {
      "type": "enumeration",
      "enum": [
        "Efectivo",
        "Tarjeta"
      ]
    },
    "TipoServicio": {
      "type": "enumeration",
      "required": true,
      "default": "Normal",
      "enum": [
        "Normal",
        "VIP",
        "Zumba",
        "Box",
        "Zumba y Box",
        "VIP + Zumba y Box"
      ]
    },
    "FechaPago": {
      "type": "date",
      "required": true
    },
    "MesPago": {
      "type": "integer",
      "required": true,
      "min": 0,
      "max": 11
    },
    "AnioPago": {
      "type": "integer",
      "required": true
    },
    "cliente": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::cliente.cliente",
      "inversedBy": "pagos"
    },
    "Turno": {
      "type": "enumeration",
      "required": false,
      "enum": [
        "de 7:00 am a 8:00 am",
        "de 8:00 am a 9:00 am",
        "de 9:00 am a 10:00 am",
        "de 10:00 am a 11:00 am",
        "de 11:00 am a 12:00 pm",
        "de 1:00 pm a 2:00 pm",
        "de 2:00 pm a 3:00 pm",
        "de 3:00 pm a 4:00 pm",
        "de 4:00 pm a 5:00 pm",
        "de 5:00 pm a 6:00 pm",
        "de 6:00 pm a 7:00 pm",
        "de 7:00 pm a 8:00 pm"
      ]
    }
  }
}
